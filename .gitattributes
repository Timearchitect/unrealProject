# Auto detect text files and perform LF normalization
* text=auto

*.uasset filter=lfs diff=lfs merge=lfs -text # unreal asset filer
*.umap filter=lfs diff=lfs merge=lfs -text #unreal map fil

*.tif filter=lfs diff=lfs merge=lfs -text #skriv in vilka filformat som ska in till LSF
*.png filter=lfs diff=lfs merge=lfs -text #bildfiormat
*.fbx filter=lfs diff=lfs merge=lfs -text
*.obj filter=lfs diff=lfs merge=lfs -text

*.wav filter=lfs diff=lfs merge=lfs -text #ljudformat
*.mp3 filter=lfs diff=lfs merge=lfs -text
*.ogg filter=lfs diff=lfs merge=lfs -text